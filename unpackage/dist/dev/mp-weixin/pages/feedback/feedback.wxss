.feedback_title {
  margin: 15px;
  margin-bottom: 0;
  font-size: 14px;
  color: #666;
}
.feedback_content {
  margin: 15px;
  padding: 10px;
  box-sizing: border-box;
  border: 1px #eee solid;
}
.feedback_content .feedback_text {
  font-size: 12px;
  width: 100%;
  height: 100px;
}
.feedback_image_box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  padding: 10px;
}
.feedback_image_box .feedback_image_item {
  position: relative;
  height: 0;
  width: 33.33%;
  padding-top: 33.33%;
  box-sizing: border-box;
}
.feedback_image_box .feedback_image_item .close_icon {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: absolute;
  right: 0;
  top: 0;
  width: 22px;
  height: 22px;
  border-radius: 50%;
  background-color: #ff5a5f;
}
.feedback_image_box .feedback_image_item .image_box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: absolute;
  top: 6px;
  bottom: 6px;
  left: 6px;
  right: 6px;
  border: 1px #eee solid;
  border-radius: 5px;
  overflow: hidden;
}
.feedback_image_box .feedback_image_item .image_box image {
  width: 100%;
  height: 100%;
}
.feedback_button {
  background-color: #F07373;
  margin: 20px;
  font-size: 16px;
}

